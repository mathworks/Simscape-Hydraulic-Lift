component(Hidden=true) TwoPort
% Hydraulic 2-Port Schema
% Copyright 2005-2012 The MathWorks, Inc.
  nodes
    A = foundation.hydraulic.hydraulic; % A:left
    B = foundation.hydraulic.hydraulic; % B:right
  end

  variables
    q = { 1 , 'm^3/s' };  % Volumetric flow rate
    p = { 0 , 'Pa' };     % Pressure
  end

  function setup
    through( q, A.q, B.q );
    across( p, A.p, B.p );
  end
end
